{
  "schemaVersion": 1,
  "name": "MatHax Client Legacy",
  "id": "mathaxlegacy",
  "version": "${version}",
  "description": "MatHax Legacy is an fork of Meteor Client, advanced Minecraft utility mod for Fabric.",
  "authors": [
    "Matejko06"
  ],
  "contact": {
    "homepage": "https://mathaxclient.xyz",
    "issues": "https://github.com/Matejko06/MatHaxClient/issues",
    "discord": "https://mathaxclient.xyz/Discord"
  },
  "license": "GPL-3.0",
  "icon": "assets/mathaxlegacy/icons/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "mathax.client.legacy.MatHaxClientLegacy"
    ]
  },
  "mixins": [
    "mathaxlegacy.mixins.json",
    "mathaxlegacy-canvas.mixins.json",
    "mathaxlegacy-sodium.mixins.json",
    "mathaxlegacy-indigo.mixins.json",
    "mathaxlegacy-baritone.mixins.json"
  ],
  "accessWidener": "mathaxlegacy.accesswidener",
  "custom": {
    "modmenu": {
      "links": {
        "Website": "https://mathaxclient.xyz",
        "Issues": "https://github.com/Matejko06/MatHaxClient-Legacy/issues",
        "Discord": "https://mathaxclient.xyz/Discord"
      }
    }
  },
  "depends": {
    "java": ">=16",
    "minecraft": ">=1.17.1"
  },
  "breaks": {
    "optifabric": "*"
  }
}
